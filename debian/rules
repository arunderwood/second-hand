#!/usr/bin/make -f

%:
	dh $@

override_dh_auto_build:
	./scripts/build-pex.sh

override_dh_auto_install:
	install -D -m 755 dist/second-hand.pex \
		debian/second-hand/usr/lib/second-hand/second-hand.pex

# Skip dwz compression - pre-compiled binaries from wheels don't have debug sections
override_dh_dwz:
